int nfi_myServer_preload(struct nfi_server *serv, char *url, char *virtual_path, char *storage_path, int opt)
{
	//char dir[NFIMAXPATHLEN];
	int ret;
	struct nfi_myServer_server *server_aux;
	struct st_myServer_msg msg;
	
#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_preload begin %s - %s \n", virtual_path, storage_path);
	fflush(stdout);
#endif
	if(url == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}

	if(url[strlen(url)-1] == '/'){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}

	if (serv == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}
	
	if (virtual_path == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}

	if (storage_path == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}
#ifdef NFI_DYNAMIC
	if (serv->private_info == NULL){
		ret = nfi_myServer_reconnect(serv);
		if(ret <0){
			/* myServer_err(); don't necessary */
			return -1;
		}
	}
#else
	if (serv->private_info == NULL){
               myServer_err(MYSERVERERR_PARAM);
               return -1;
        }
#endif

	server_aux = (struct nfi_myServer_server *) serv->private_info;
#ifdef DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_preload: preload %s in server %s.\n",virtual_path,serv->server);
	fflush(stdout);
#endif
	
	/*****************************************/
	msg.type = MYSERVER_PRELOAD_FILE; 
	//strcpy(msg.u_st_myServer_msg.op_preload.path,dir);
	strcpy(msg.u_st_myServer_msg.op_preload.virtual_path,virtual_path);
	strcpy(msg.u_st_myServer_msg.op_preload.storage_path,storage_path);
	msg.u_st_myServer_msg.op_preload.opt = opt;

#ifdef DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_preload: sizeof(struct st_myServer_msg) = %d.\n", sizeof(struct st_myServer_msg));
	fflush(stdout);
#endif	
        myServer_write_operation(server_aux->sd, &msg);
#ifdef DBG_NFI 
	fflush(stdout);
	printf("preload: -> %s \n",msg.u_st_myServer_msg.op_preload.virtual_path);
	fflush(stdout);
#endif
	myServer_read_data(server_aux->sd, (char *)&ret, sizeof(int));
#ifdef DBG_NFI 
	fflush(stdout);
	printf("preload: <- %d \n",ret);
	fflush(stdout);
#endif
	/*****************************************/
	
#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_preload end %s - %s = %d\n", virtual_path, storage_path, ret);
	fflush(stdout);
#endif
	if(ret == -1){
		printf("Error en el preload\n");
	}

	return ret;
}


