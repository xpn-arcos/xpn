	return 0;
}



int nfi_myServer_open(struct nfi_server *serv,  char *url, struct nfi_fhandle *fho )
{
	char dir[NFIMAXPATHLEN], server[NFIMAXPATHLEN];
	int ret;
	struct nfi_myServer_server *server_aux;
	struct nfi_myServer_fhandle *fh_aux;

	struct st_myServer_msg msg;
	
#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_open begin %s\n",url);
	fflush(stdout);
#endif
	if(url[strlen(url)-1] == '/'){
		return nfi_myServer_opendir(serv, url, fho);
	}
	
	if (serv == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}
	
	if (fho == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}
#ifdef NFI_DYNAMIC
	if (serv->private_info == NULL){
		ret = nfi_myServer_reconnect(serv);
		if(ret <0){
			/* myServer_err(); don't necessary */
			return -1;
		}
	}
#else
	if (serv->private_info == NULL){
               myServer_err(MYSERVERERR_PARAM);
               return -1;
        }
#endif

	ret = ParseURL(url, NULL, NULL, NULL, server,  NULL,  dir);
	if(ret < 0){
		fprintf(stderr,"nfi_myServer_open: url %s incorrect.\n",url);
		myServer_err(MYSERVERERR_URL);
		return -1;
	}
	fho->url = (char *)malloc(strlen(url)+1);
	if(fho->url == NULL){
		myServer_err(MYSERVERERR_MEMORY);
		return -1;
	}

	strcpy(fho->url, url);

	fh_aux = (struct nfi_myServer_fhandle *)malloc(sizeof(struct nfi_myServer_fhandle));
	if (fh_aux == NULL){		
		myServer_err(MYSERVERERR_MEMORY);
		free(fho->url);
		return -1;
	}

	server_aux = (struct nfi_myServer_server *) serv->private_info;
	
	/*****************************************/
	msg.type = MYSERVER_OPEN_FILE; 
	strcpy(msg.u_st_myServer_msg.op_open.path,dir);
#ifdef DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_open: sizeof(istruct st_myServer_msg) = %d.\n", sizeof(struct st_myServer_msg));
	fflush(stdout);
#endif	
	
        myServer_write_operation(server_aux->sd, &msg);
#ifdef DBG_NFI 
	fflush(stdout);
	printf("open: -> %s \n",msg.u_st_myServer_msg.op_open.path);
	fflush(stdout);
#endif
	myServer_read_data(server_aux->sd, (char *)&fh_aux->fd, sizeof(int));
#ifdef DBG_NFI 
	fflush(stdout);
	printf("open: <- %d \n",fh_aux->fd);
	fflush(stdout);
#endif
	strcpy(fh_aux->path, dir);
	/*****************************************/
	
	fho->type = NFIFILE;			
	
	fho->server = NULL;
	fho->priv_fh = NULL;
	fho->server = serv;
	fho->priv_fh = (void *) fh_aux;

#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_open end\n");
	fflush(stdout);
#endif
	return 0;

}





int nfi_myServer_close(struct nfi_server *server,  struct nfi_fhandle *fh){
	
	struct nfi_myServer_fhandle *fh_aux;
	struct nfi_myServer_server *server_aux;
	struct st_myServer_msg msg;

#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_close begin\n");
	fflush(stdout);
#endif
	if (server == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}
	if (fh == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}
	
	if (fh->priv_fh != NULL){
		fh_aux = (struct nfi_myServer_fhandle *) fh->priv_fh;
		server_aux = (struct nfi_myServer_server *) server->private_info;
		/*****************************************/
		msg.type = MYSERVER_CLOSE_FILE; 
		msg.u_st_myServer_msg.op_close.fd = fh_aux->fd;
	
#ifdef DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_close: sizeof(struct st_myServer_msg) = %d.\n", sizeof(struct st_myServer_msg));
	fflush(stdout);
#endif	
        myServer_write_operation(server_aux->sd, &msg);
			
#ifdef  DBG_NFI 
	fflush(stdout);
		printf("close: -> %d \n",msg.u_st_myServer_msg.op_close.fd);
	fflush(stdout);
#endif
		/*****************************************/
		//close(fh_aux->fd);
		/* free memory */
		free(fh->priv_fh);
		fh->priv_fh = NULL;
	}
	
	fh->type = NFINULL;
	fh->server = NULL;
#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_close end\n");
	fflush(stdout);
#endif

	return 0;
}

ssize_t nfi_myServer_read(	struct nfi_server *serv,
			struct nfi_fhandle *fh,
			void *buffer,
			off_t offset,
			size_t size)
			//off_t offset)
{

	int ret, cont;

	struct nfi_myServer_server *server_aux;
	struct nfi_myServer_fhandle *fh_aux;
	struct st_myServer_msg msg;
	struct st_myServer_read_req req;

#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_read begin off %d size %d\n",(int)offset, (int)size);
	fflush(stdout);
#endif
	if (serv == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}


	if (fh == NULL){
		myServer_err(MYSERVERERR_PARAM);
		return -1;
	}


#ifdef NFI_DYNAMIC
	if (serv->private_info == NULL){
		ret = nfi_myServer_reconnect(serv);
		if(ret <0){
			/* myServer_err(); don't necessary */
			return -1;
		}
	}
#else
	if (serv->private_info == NULL){
               myServer_err(MYSERVERERR_PARAM);
               return -1;
        }
#endif


	fh_aux = (struct nfi_myServer_fhandle *) fh->priv_fh;
	server_aux = (struct nfi_myServer_server *) serv->private_info;
	
	/*****************************************/
		
	msg.type = MYSERVER_READ_FILE; 
	msg.u_st_myServer_msg.op_read.fd 	= fh_aux->fd;
	msg.u_st_myServer_msg.op_read.offset 	= offset;
	msg.u_st_myServer_msg.op_read.size 	= size;

	#ifdef  DBG_IO
	fflush(stdout);
		printf("read: -> fd %d \n",msg.u_st_myServer_msg.op_read.fd);
		printf("read: -> offset %d \n",(int)msg.u_st_myServer_msg.op_read.offset);
		printf("read: -> size %d \n",msg.u_st_myServer_msg.op_read.size);
	fflush(stdout);
	#endif  

#ifdef DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_read: sizeof(struct st_myServer_msg) = %d.\n", sizeof(struct st_myServer_msg));
	fflush(stdout);
#endif	

	ret = myServer_write_operation(server_aux->sd, &msg);
	if(ret == -1){
		fprintf(stderr,"(1)nfi_myServer_read: Error on read operation\n");
		return -1;
	}

	cont = 0;
	do{
		ret = myServer_read_data(server_aux->sd, (char *)&req, sizeof(struct st_myServer_read_req));
#ifdef DBG_NFI 
		printf("nfi_myServer_read: (1)myServer_read_data = %d.\n", ret);
#endif		
		if(ret == -1){
			fprintf(stderr,"(1)nfi_myServer_read: Error on read operation\n");
			return -1;
		}


		if(req.size > 0){
			ret = myServer_read_data(server_aux->sd, (char *)buffer+cont, req.size);
#ifdef DBG_NFI 
			printf("nfi_myServer_read: (2)myServer_read_data = %d.\n", ret);
#endif		
			if(ret == -1){
				fprintf(stderr,"(2)nfi_myServer_read: Error on read operation\n");
				return -1;
			}
			//cont += req.size;
		}
		cont += req.size;
#ifdef  DBG_NFI 
	fflush(stdout);
		printf("read(%d,%d) cont = %d\n",req.size,(int)req.last,cont);	
	fflush(stdout);
#endif
	}while((req.size>0)&&(!req.last));
	
	/*****************************************/
#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_read: read %s off %d size %d (err:%d).\n",fh->url,(int)offset,(int)size,(int)req.size);
	fflush(stdout);
#endif
	if(req.size < 0){
		fprintf(stderr,"nfi_myServer_read: Fail read %s off %d size %d (err:%d).\n",fh->url,(int)offset,(int)size,(int)req.size);
		myServer_err(MYSERVERERR_READ);
		return -1;
	}

#ifdef  DBG_NFI 
	fflush(stdout);
	printf("nfi_myServer_read end\n");
	fflush(stdout);
#endif
	return req.size;
}



