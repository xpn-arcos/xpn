/**********************************
Write the operation to realize
***********************************/
int myServer_write_operation(int sd, struct st_myServer_msg *head){
        int ret;


#ifdef _DBG_
	printf("ID=%s)myServer_write_data: begin\n",head->id);	
#endif
        ret = myServer_write_data(sd, (char *)&(head->type), sizeof(int));
        if(ret == -1){
                return -1;
        }


#ifdef _DBG_
        printf("ID=%s)antes de write_operation: %d\n ", head->id, head->type);
#endif
        ret = myServer_write_data(sd, (char *)&(head->id), sizeof(int));
        if(ret == -1){
                return -1;
        }

#ifdef _DBG_
	
        printf("ID=%s)write_operation: %d -> \n", head->id, head->type);
#endif
        switch(head->type){
case MYSERVER_OPEN_FILE:
#ifdef _DBG_
        printf("sd=%d,id=%d)OPEN operation\n",sd,head->id);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_open), sizeof(struct st_myServer_open));
        if(ret == -1){
                return -1;
        }
        break;
case MYSERVER_CREAT_FILE:
#ifdef _DBG_
        printf("sd=%d,id=%d)CREAT operation\n",sd,head->id);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_creat), sizeof(struct st_myServer_creat));
        if(ret == -1){
                return -1;
        }
        break;
case MYSERVER_READ_FILE:
#ifdef _DBG_
        printf("sd=%d,id=%d)READ operation\n", sd,head->type);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_read), sizeof(struct st_myServer_read));
        if(ret == -1){
                return -1;
        }
        break;
case MYSERVER_WRITE_FILE:
#ifdef _DBG_
        printf("sd=%d,id=%d)WRITE operation\n",sd, head->type);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_write), sizeof(struct st_myServer_write));
        if(ret == -1){
                return -1;
        }
        break;
case MYSERVER_CLOSE_FILE:
#ifdef _DBG_
        printf("sd=%d,id=%d)CLOSE operation\n", sd,head->type);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_close), sizeof(struct st_myServer_close));
        if(ret == -1){
                return -1;
        }
        break;
case MYSERVER_RM_FILE:
#ifdef _DBG_
        printf("sd=%d,id=%d)RM operation\n", sd,head->type);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_rm), sizeof(struct st_myServer_rm));
        if(ret == -1){
                return -1;
        }
        break;
case MYSERVER_GETATTR_FILE:
#ifdef _DBG_
        printf("sd=%d,id=%d)GETATTR operation\n", sd,head->type);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_getattr), sizeof(struct st_myServer_getattr));
        if(ret == -1){
                return -1;
        }
        break;
case MYSERVER_MKDIR_DIR:
#ifdef _DBG_
        printf("sd=%d,id=%d)MDKIR operation\n", sd,head->type);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_mkdir), sizeof(struct st_myServer_mkdir));
        if(ret == -1){
                return -1;
        }
        break;
case MYSERVER_RMDIR_DIR:
#ifdef _DBG_
        printf("sd=%d,id=%d)RMDIR operation\n", sd,head->type);
#endif
        ret = myServer_write_data(sd, (char *)&(head->u_st_myServer_msg.op_rmdir), sizeof(struct st_myServer_rmdir));
        if(ret == -1){
                return -1;
        }
