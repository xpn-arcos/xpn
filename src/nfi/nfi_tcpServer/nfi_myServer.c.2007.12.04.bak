        myServer_write_operation(server_aux->sd, &msg);
#ifdef DBG_NFI 
	fflush(stdout);
	printf("flush: -> %s \n",msg.u_st_myServer_msg.op_flush.virtual_path);
	fflush(stdout);
#endif
	myServer_read_data(server_aux->sd, (char *)&ret, sizeof(int));
